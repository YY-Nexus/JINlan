      "src": "/images/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable any"
    }
  ],
  "shortcuts": [
    {
      "name": "客户管理",
      "short_name": "客户",
      "description": "管理客户信息和服务记录",
      "url": "/customers",
      "icons": [
        {
          "src": "/images/icons/customers-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "任务管理",
      "short_name": "任务",
      "description": "查看和管理待办任务",
      "url": "/tasks",
      "icons": [
        {
          "src": "/images/icons/tasks-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "数据分析",
      "short_name": "分析",
      "description": "查看业务数据和报表",
      "url": "/analytics",
      "icons": [
        {
          "src": "/images/icons/analytics-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "通知中心",
      "short_name": "通知",
      "description": "查看系统通知和消息",
      "url": "/notifications",
      "icons": [
        {
          "src": "/images/icons/notifications-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/images/screenshots/desktop-dashboard.png",
      "sizes": "1280x720",
      "type": "image/png",
      "platform": "wide",
      "label": "Dashboard view on desktop"
    },
    {
      "src": "/images/screenshots/mobile-dashboard.png",
      "sizes": "375x667",
      "type": "image/png",
      "platform": "narrow",
      "label": "Dashboard view on mobile"
    },
    {
      "src": "/images/screenshots/desktop-customers.png",
      "sizes": "1280x720",
      "type": "image/png",
      "platform": "wide",
      "label": "Customer management on desktop"
    },
    {
      "src": "/images/screenshots/mobile-customers.png",
      "sizes": "375x667",
      "type": "image/png",
      "platform": "narrow",
      "label": "Customer management on mobile"
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://care.jinlan.com/manifest.json"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/compose?to=%s"
    },
    {
      "protocol": "tel",
      "url": "/call?number=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "text/plain", ".csv", ".xlsx"]
        }
      ]
    }
  }
}
